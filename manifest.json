{
    "name": "Quick Playlister",
    "description": "Create playlist for your temperory youtube session.",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
      },
      "permissions": ["tabs","scripting", "activeTab", "contextMenus", "storage"],
      "icons": {
        "16": "16-icon.png",
        "48": "48-icon.png"
      },
      "action": {
        "default_popup": "popup.html"
      },
      "options_page": "options.html"
  }