{
    "name": "Quick Floating Playlist",
    "description": "Create and Play playlist quickly from anywhere in browser and never forgot to watch watch later videos.",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
      },
      "permissions": ["tabs", "contextMenus", "storage"],
      "icons": {
        "16": "16-icon.png",
        "48": "48-icon.png"
      },
      "action": {
        "default_popup": "popup.html"
      },
      "options_page": "options.html"
  }